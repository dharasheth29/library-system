{
  "name": "library-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && mkdir -p ../../build/apps/library-ui && cp -r build/* ../../build/apps/library-ui && rm -rf build",
    "test": "react-scripts test --silent --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-jest": "^29.7.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hooks": "^1.0.1",
    "react-router-dom": "^6.22.2",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.8",
    "typescript": "^3.2.1",
    "web-vitals": "^2.1.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx"
    ],
    "transform": {
      "^.+\\.tsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "../../node_modules/(?!ky)"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/apps/library-ui/src/$1"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.0-placeholder-for-preset-env.2",
    "@babel/preset-react": "^7.23.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/testing-library__jest-dom": "^6.0.0",
    "jest": "^27.5.1"
  }
}
